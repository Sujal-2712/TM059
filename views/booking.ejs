    <script>
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();

                const targetId = this.getAttribute('href').substring(1);
                const targetElement = document.getElementById(targetId);

                if (targetElement) {
                    window.scrollTo({
                        top: targetElement.offsetTop,
                        behavior: 'smooth'
                    });
                }
            });
        });
        document.getElementById('myForm').onsubmit = async function (event) {
            document.getElementById('direction-btn').disabled = false;

            event.preventDefault();
            let formData = new FormData(document.getElementById("myForm"));
            var symptoms = [];
            if (document.getElementById('diabetes').checked) symptoms.push("Diabetes");
            if (document.getElementById('BP').checked) symptoms.push("Blood Pressure");
            if (document.getElementById('breathing').checked) symptoms.push("Breathing Problem");
            if (document.getElementById('bleeding').checked) symptoms.push("Bleeding Problem");

            const locationText = document.getElementById('current-location').innerText;
            let trimmedText = locationText.replace("Your Location is ", "");
            

            const response = await fetch('/addPatientdata', {
                method: 'POST',
                body: JSON.stringify({ 'location': trimmedText, 'symptoms': symptoms }),
                headers: {
                    "Content-Type": "application/json",
                }
            });
        };


        function getLocation() {
            let req = new XMLHttpRequest();

            req.open("GET", "/current-location", true);

            req.onreadystatechange = function () {
                if (req.readyState == 4 && req.status == 200) {
                    const locationData = JSON.parse(req.responseText);
                    console.log(locationData);
                    const displayText = ` Your Location is
                 ${locationData.city},
                 ${locationData.postal},
                ${locationData.region},
                 ${locationData.country}
                
            `;
                    const [lat, lon] = locationData.loc.split(',');
                    initMap(lat, lon);
                    document.getElementById('current-location').innerHTML = displayText;
                }
            }
            req.send();
        }

        function initMap(lat, lon) {
            console.log('Received coordinates:', lat, lon);

            var map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: parseFloat(lat), lng: parseFloat(lon) },
                zoom: 8
            });

            var marker = new google.maps.Marker({
                position: { lat: parseFloat(lat), lng: parseFloat(lon) },
                map: map,
                title: 'Your Marker Title',
                icon: {
                    url: 'http://maps.google.com/mapfiles/ms/icons/red-dot.png',
                    scaledSize: new google.maps.Size(40, 40)
                }
            });
        }

        window.addEventListener('load', () => {
            getLocation();
            loadProfile()
        });
        function loadProfile() {
            let req = new XMLHttpRequest();
            req.open("GET", "/profile", true);

            req.onreadystatechange = function () {
                if (req.readyState == 4 && req.status == 200) {
                    console.log(req.responseText);

                    // Parse the JSON response
                    let responseData = JSON.parse(req.responseText);

                    // Access properties using responseData
                    document.getElementById('username').innerHTML = responseData.name;
                    document.getElementById('useremail').innerHTML = responseData.email;
                }
            };

            req.send();

        }

    </script>
